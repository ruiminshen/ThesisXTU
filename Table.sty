\usepackage[round-precision = 5, round-mode = figures]{siunitx}
\newcommand{\CellFormat}[2]{
\setlength{\tabcolsep}{0pt}
\begin{tabular}{c}
\num{#1}\tabularnewline
\hline 
\num{#2}\tabularnewline
\end{tabular}
}

